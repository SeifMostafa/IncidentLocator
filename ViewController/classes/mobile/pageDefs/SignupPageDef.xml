        <Item Value="password"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="Signup" RequiresUpdateModel="true" Action="invokeMethod" MethodName="Signup"
                  IsViewObjectMethod="false" DataControl="Account" InstanceName="data.Account.dataProvider"/>
  </bindings>
</pageDefinition>
